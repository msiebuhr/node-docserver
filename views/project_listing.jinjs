{% extends "layout.jinjs" %}

{% block title %}Documentation{% endblock %}

{% block body %}

<h2>Projects</h2>
{% if projects %}
<ul class="lead">
{% for project in projects %}
	<li><a href="/{{ project }}">{{ project }}</a></li>
{% endfor %}
</ul>
{% else %}
<p class="alert alert-error">No documentation found for this project!</p>
{% endif %}

{% endblock %}

